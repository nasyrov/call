{
  "name": "call",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^22.21.0",
    "pnpm": "^10.19.0"
  },
  "packageManager": "pnpm@10.19.0",
  "scripts": {
    "build": "next build",
    "worker:build": "tsup src/worker/index.ts --format esm --target node22 --outDir .next/worker --clean",
    "worker:dev": "dotenv -e .env -- tsx src/worker/index.ts",
    "worker:start": "node .next/worker/index.js",
    "db:auth": "dotenv -e .env -o -- pnpm dlx @better-auth/cli generate --config ./src/server/better-auth/config.ts --output ./src/server/db/schema/auth.ts -y",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "dev": "next dev --turbo",
    "format": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:fix": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "knip": "SKIP_ENV_VALIDATION=true knip",
    "lint": "SKIP_ENV_VALIDATION=1 next lint",
    "lint:fix": "SKIP_ENV_VALIDATION=1 next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.956.0",
    "@aws-sdk/s3-request-presigner": "^3.948.0",
    "@hookform/resolvers": "^5.2.2",
    "@livekit/components-react": "^2.9.17",
    "@livekit/components-styles": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tanstack/react-query": "^5.90.12",
    "better-auth": "^1.3",
    "bullmq": "^5.66.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.45.1",
    "livekit-client": "^2.16.1",
    "livekit-server-sdk": "^2.15.0",
    "lucide-react": "^0.561.0",
    "next": "15.5.9",
    "next-themes": "^0.4.6",
    "postgres": "^3.4.4",
    "react": "^19.0.0",
    "react-day-picker": "^9.12.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.68.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zod": "^3.24.2",
    "zsa": "^0.6.0",
    "zsa-react-query": "^0.2.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@tailwindcss/postcss": "^4.0.15",
    "@types/node": "^20.14.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "dotenv-cli": "^11.0.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.23.0",
    "eslint-config-next": "15.5.9",
    "eslint-plugin-drizzle": "^0.2.3",
    "knip": "^5.73.4",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.0.15",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0"
  }
}
